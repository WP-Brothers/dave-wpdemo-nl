{
  "name": "sbtweema",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint --ignore-path .gitignore --ext .ts .",
    "eslint:fix": "eslint --ignore-path .gitignore --ext .ts . --fix",
