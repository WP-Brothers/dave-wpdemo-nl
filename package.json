  "lint-staged": {
    "*.ts": [
      "prettier --config .prettierrc './wp-content/themes/socialbrothers' --write",
      "eslint --ignore-path .gitignore --ext .ts . --fix"
    ],
    "*.scss": "stylelint --fix",
    "*.php": "composer run format"
  },
  "devDependencies": {
